{
  "name": "hegland/text-parser",
  "type": "library",
  "version": "2.0.2",
  "description": "A Text parser. It allows you to cut out the needed part of a text from a given text.",
  "keywords": [
    "parser"
  ],
  "homepage": "https://github.com/rogerhegland/text-parser",
  "license": "MIT",
  "authors": [
    {
      "name": "Roger Hegland"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3.0"
  },
  "autoload": {
    "psr-4": {
      "TextParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TextParser\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
