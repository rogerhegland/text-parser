{
  "name": "hegland/text-parser",
  "type": "library",
  "version": "1.0.0",
  "description": "An Text parser. It allows you to cut the needed part of a text in given text.",
  "keywords": [
    "parser"
  ],
  "homepage": "https://github.com/rogerhegland/text-parser",
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "Roger Hegland",
      "email": "paquettg@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3.0"
  },
  "autoload": {
    "psr-4": {
      "TextParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TextParser\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
